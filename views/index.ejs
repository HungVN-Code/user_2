<!DOCTYPE html>
<html>
  <head>
    <title>USER2</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>Danh sách user2</h1>
    <a class="add" href="form_add"><button>Thêm user</button></a><br>

    <table border="1">
      <tr>
        <th>ID</th>
        <th>ENABLEFCM</th>
        <th>AVATAR</th>
        <th>BANNER</th>
        <th>FULLNAME</th>
        <th>TYPEUSER</th>
        <th>IDPROVINCE</th>
        <th>ACTION</th>
      </tr>
      <% users.forEach(item => { %>
        <tr>
          <td><%= item._id %> </td>
          <td><%= item.enablefcm %> </td>
          <td><img src="<%= item.avatar %>" alt="<%= item.banner %>"> </td>
          <td><img src="<%= item.banner %>" alt="<%= item.banner %>"> </td>
          <td><%= item.fullname %> </td>
          <td><%= item.typeuser %> </td>
          <td><%= item.idprovince %> </td>
          <td><a href="/form_update/<%= item._id %>">Update</a>&#160;&#160;&#160;||&#160;&#160;&#160;<a href="/form_delete/<%= item._id %>">Delete</a></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>
